{
  "name": "Recipes",
  "version": "0.1.0",
  "homepage": "https://volodymyr-kushnir.github.io/recipes/",
  "private": true,
  "devDependencies": {
    "gh-pages": "1.0.0",
    "husky": "0.14.3",
    "lint-staged": "4.2.3",
    "prettier": "1.7.4",
    "react-scripts": "1.0.14",
		"source-map-explorer": "1.5.0"
  },
  "dependencies": {
    "classnames": "2.2.5",
    "lato-font": "3.0.0",
    "moment": "2.18.1",
    "normalizr": "3.2.3",
    "query-string": "5.0.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-router": "4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
		"redux-persist": "5.0.0-beta.1",
    "redux-saga": "0.15.6",
    "redux-thunk": "2.2.0",
    "semantic-ui-react": "0.74.2",
    "uuid": "3.1.0"
  },
  "scripts": {
		"analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
		"format": "prettier --trailing-comma es5 --print-width 100 --tab-width 2 --single-quote --write 'src/**/*.js'",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --trailing-comma es5 --print-width 100 --tab-width 2 --single-quote --write",
      "git add"
    ]
  }
}
